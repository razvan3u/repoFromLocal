node {
    stage('create'){
        parallel(
        newFile: {
               node{
               sh 'touch newFile'
        }
        cloneRepo: {
               node{
               git branch: 'main', url: 'https://github.com/razvan3u/newRepoOnGH'
        }
    }
    stage('write'){
        sh 'echo "some text to this new file" > newFile'
    }
    stage('print'){
        sh 'cat newFile'
    }
    stage('print feature1 file from repo'){
        sh 'cat feature1'
    }
}
